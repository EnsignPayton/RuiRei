<rui:ReactiveUserControl
    x:Class="RuiRei.Wpf.Views.SenseView"
    x:TypeArguments="vm:SenseViewModel"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:rui="http://reactiveui.net"
    xmlns:vm="clr-namespace:RuiRei.Reactive.ViewModels;assembly=RuiRei.Reactive"
    mc:Ignorable="d"
    d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel Margin="0 4">
        <TextBlock x:Name="PartsOfSpeechBlock"
                   FontSize="10"
                   Foreground="DimGray"/>
        <TextBlock x:Name="MeaningsBlock"
                   FontSize="16" />
    </StackPanel>
</rui:ReactiveUserControl>
